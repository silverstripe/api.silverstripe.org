{
    "name": "silverstripe/api.silverstripe.org",
    "description": "API documentation for the SilverStripe Framework",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Stephen Shkardoon",
            "email": "stephen@silverstripe.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "silverstripe/recipe-core": "^6",
        "silverstripe/supported-modules": "^2",
        "silverstripe/totp-authenticator": "^6",
        "symbiote/silverstripe-queuedjobs": "^6",
        "gitonomy/gitlib": "^1",
        "symfony/console": "^7",
        "code-lts/doctum": "dev-main#40df854fd30212c7dc8cbeb070fdf0679d400732",
        "psr/log": "^3"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\ApiDocs\\": "app/src/",
            "SilverStripe\\ApiDocs\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11",
        "squizlabs/php_codesniffer": "^3"
    },
    "extra": {
        "project-files-installed": [
            ".htaccess",
            "app/.htaccess",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "config": {
        "platform": {
            "php": "8.3"
        },
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
